{
  "name": "0xgasless-swap-microservice",
  "version": "1.0.0",
  "description": "Production-ready microservice for 0xGasless Smart Swap functionality",
  "main": "dist/server.js",
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target node",
    "start": "NODE_ENV=production bun ./dist/server.js",
    "start:dev": "bun run src/server.ts",
    "type-check": "tsc --noEmit",
    "test": "./test-microservice.sh",
    "test:unit": "bun test",
    "lint": "echo 'Linting with built-in bun tools'",
    "clean": "rm -rf dist logs/*.log",
    "logs": "tail -f logs/combined.log",
    "health-check": "curl -f http://localhost:3002/health || exit 1"
  },
  "dependencies": {
    "@0xgasless/agentkit": "^0.0.16",
    "@langchain/core": "^0.3.2",
    "@langchain/langgraph": "^0.2.17",
    "@langchain/openai": "^0.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "bun-types": "latest"
  },
  "keywords": ["0xgasless", "swap", "microservice", "defi", "blockchain", "production"],
  "author": "Mason",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  }
}
